<%= render "/shared/banner", name: @list.name, image: @list.image_url %>

<ul>
  <% @list.bookmarks.each do |bookmark| %>
    <li>
      <%= bookmark.movie.title %> -
      <%= link_to "<i class='fa-solid fa-trash-can'></i>".html_safe, bookmark_path(bookmark), data: { turbo_method: :delete } %>
    </li>
  <% end %>
</ul>
